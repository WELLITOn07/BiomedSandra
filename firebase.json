{
  "hosting": {
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "script-src 'self' https://www.googletagmanager.com; connect-src 'self' https://www.google-analytics.com;"
          }
        ]
      }
    ]
  },
    "public": "dist/biomed-sandra/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }